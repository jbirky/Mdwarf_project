	Issn = {15384357},
	Journal = {Astrophysical Journal},
	Keywords = {abundances - stars,fundamental parameters - techniques,spectroscopic,stars},
	Number = {1},
	Title = {{Stellar chemical abundances: In pursuit of the highest achievable precision}},
	Volume = {795},
	Year = {2014},
	Bdsk-Url-1 = {https://doi.org/10.1088/0004-637X/795/1/23}}

