	Issn = {09354956},
	Journal = {Astronomy and Astrophysics Review},
	Keywords = {Galaxy: bulge,Galaxy: disk,Galaxy: evolution,Galaxy: halo,Planet--star interactions,Stars: abundances,Stars: fundamental parameters,Techniques: spectroscopic},
	Number = {1},
	Title = {{High-precision stellar abundances of the elements: methods and applications}},
	Volume = {26},
	Year = {2018},
	Bdsk-Url-1 = {https://doi.org/10.1007/s00159-018-0111-3}}

